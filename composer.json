{
    "name": "malbrandt/laravel-trace",
    "description": "An elegant way to append trace to your logic and data.",
    "type": "library",
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Marek Malbrandt",
            "email": "marek.malbrandt@gmail.com"
        }
    ],
    "config": {
        "platform": {
            "php": "7.0.0"
        }
    },
    "require": {
        "laravel/laravel": ">=5.0"
    },
    "autoload": {
        "psr-4": {
            "Malbrandt\\Laravel\\Trace\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Malbrandt\\Laravel\\Trace\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "orchestra/testbench": "^3.5",
        "phpunit/phpunit": "^6.5",
        "orchestra/database": "^3.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Malbrandt\\Laravel\\Trace\\Provider\\TraceServiceProvider"
            ]
        }
    }
}
